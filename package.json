{
  "name": "react-face-recognition",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.26.0",
    "axios": "^0.19.0",
    "babel-jest": "^24.9.0",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-loader-icon": "^1.0.1",
    "react-scripts": "3.2.0",
    "tachyons": "^4.11.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./jest-helpers/jest-setup.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.(png)$": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|serverscss|jpg|jpeg|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|inserted|insertedpng)$": "identity-obj-proxy",
      "worker-loader!.*": "<rootDir>/src/__mocks__/workerMock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 62,
        "functions": 70,
        "lines": 71,
        "statements": 67
      }
    },
    "globals": {
      "RELEASE_VERSION": "TEST_RELEASE_VERSION"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "^1.19.1"
  }
}
